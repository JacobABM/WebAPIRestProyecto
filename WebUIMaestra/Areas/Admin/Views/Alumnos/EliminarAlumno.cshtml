@model WebUIMaestra.Models.Usuario
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "EliminarAlumno";
}

<h1>Eliminar un Alumno</h1>
<form>
    <input type="text" asp-for="Id" hidden readonly/>
    <label>¿Desea eliminar a @Model.Nombre?</label>
    <input type="submit" />
</form>

